<http://fixme.com#GrafShape>
	a sh:NodeShape ;
	sh:targetClass <http://fixme.com#Graf> ;
	sh:property [
		sh:name "grafteken" ;
		sh:description "Duidt aan of er al dan niet een grafteken, bijvoorbeeld een zerk, aanwezig is." ;
		sh:path <http://fixme.com#grafteken> ;
		sh:datatype <http://www.w3.org/2001/XMLSchema#boolean> ;
		sh:minCount 1 ;
		sh:maxCount 1 ;
	] ;
	sh:property [
		sh:name "capaciteit" ;
		sh:description "Geeft het aantal beschikbare plaatsen van een graf weer." ;
		sh:path <http://data.vlaanderen.be/ns/openbaardomein/infrastructuur#capaciteit> ;
		sh:datatype <http://www.w3.org/2001/XMLSchema#integer> ;
		sh:minCount 1 ;
		sh:maxCount 1 ;
	] ;
	sh:property [
		sh:name "bezetting" ;
		sh:description "Geeft het aantal bezette plaatsen van een graf weer." ;
		sh:path <http://fixme.com#bezetting> ;
		sh:datatype <http://www.w3.org/2001/XMLSchema#integer> ;
		sh:minCount 1 ;
		sh:maxCount 1 ;
	] ;
	sh:property [
		sh:name "grafkelderAanwezig" ;
		sh:description "Duidt aan of het graf voorzien is van een ondergrondse of bovengrondse ruimte." ;
		sh:path <http://fixme.com#grafkelderAanwezig> ;
		sh:datatype <http://www.w3.org/2001/XMLSchema#boolean> ;
		sh:minCount 1 ;
		sh:maxCount 1 ;
	] ;
	sh:property [
		sh:name "graftype" ;
		sh:description "Het type graf." ;
		sh:path <http://fixme.com#graftype> ;
		sh:class <http://fixme.com#Graftypes> ;
		sh:minCount 1 ;
		sh:maxCount 1 ;
	] ;
	sh:closed false .

