		sh:name "geometrie" ;
		sh:description "De geometrie overeenkomstig met een bepaald object." ;
		sh:path <http://www.w3.org/ns/locn#geometry> ;
		sh:class <http://www.opengis.net/ont/geosparql#Geometry> ;
		sh:minCount 1 ;
		sh:maxCount 1 ;
	] ;
	sh:closed false .

