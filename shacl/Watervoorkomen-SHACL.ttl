@prefix sh:      <http://www.w3.org/ns/shacl#> .
<http://data.vlaanderen.be/ns/openbaardomein#FysiekVoorkomenShape>
	a sh:NodeShape ;
	sh:targetClass <http://data.vlaanderen.be/ns/openbaardomein#FysiekVoorkomen> ;
	sh:closed false .

<http://fixme.com#WaterVoorkomenShape>
	a sh:NodeShape ;
	sh:targetClass <http://fixme.com#WaterVoorkomen> ;
	sh:property [
		sh:name "watertype" ;
		sh:description "Het type water." ;
		sh:path <http://fixme.com#watertype> ;
		sh:class <http://www.w3.org/2004/02/skos/core#Concept> ;
		sh:minCount 1 ;
		sh:maxCount 1 ;
	] ;
	sh:closed false .

