	sh:closed false .

<http://fixme.com#DiepWaterMetOevervegetatieShape>
	a sh:NodeShape ;
	sh:targetClass <http://fixme.com#DiepWaterMetOevervegetatie> ;
	sh:closed false .

<http://fixme.com#OndiepWaterShape>
	a sh:NodeShape ;
	sh:targetClass <http://fixme.com#OndiepWater> ;
	sh:closed false .

