      dcterms:title "Public Service - Transaction"@en , "Dienst - Transactie"@nl ;
      vann:preferredNamespaceUri
              "http://data.vlaanderen.be/ns/dienst" .

<http://data.vlaanderen.be/ns/dienst#OutputType>
      a       owl:Class ;
      rdfs:comment "TODO"@nl ;
