<http://data.vlaanderen.be/ns/weg#linkerstraatnaam>
      a       owl:ObjectProperty ;
      rdfs:comment "Straatnaam voorkomend aan de linkerstraatkant"@nl ;
      rdfs:domain <http://data.vlaanderen.be/ns/weg#Wegsegment> ;
