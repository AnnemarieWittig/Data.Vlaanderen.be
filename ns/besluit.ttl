    rdfs:isDefinedBy <http://data.vlaanderen.be/ns/besluit> ;
    rdfs:range <http://data.vlaanderen.be/ns/besluit#Agendapunt> .

<http://data.vlaanderen.be/ns/besluit#gebeurtNa> a owl:ObjectProperty ;
    rdfs:label "gebeurt na"@nl ;
