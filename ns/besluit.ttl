<http://data.vlaanderen.be/ns/besluit#onderwerp> a owl:DatatypeProperty ;
    rdfs:label "onderwerp"@nl ;
    rdfs:comment "De beschrijving van het onderwerp waarover de stemming gaat."@nl ;
