    rdfs:range <http://data.vlaanderen.be/ns/mandaat#Mandataris> ;
    rdfs:subPropertyOf <http://data.vlaanderen.be/ns/besluit#heeftStemmer> .

<http://data.vlaanderen.be/ns/besluit#heeftVoorzitter> a owl:ObjectProperty ;
