odo:BedekkingOnbegroeidVoorkomen
      a       owl:Class ;
      rdfs:comment "Niet gecompacteerd en op niet-duurzame wijze verankerd met het"@nl ;
      rdfs:isDefinedBy <http://data.vlaanderen.be/ns/openbaardomein/onbegroeidvoorkomen> ;
      rdfs:label "Bedekking Onbegroeid Voorkomen"@nl ;
      rdfs:subClassOf odo:OnbegroeidVoorkomen ;
      vann:usageNote "Naar GRB-skeletaanvulling detail: WSV1: Soort verharding/bedekking."@nl .

odo:BestratingOnbegroeidVoorkomen
      a       owl:Class ;
      rdfs:comment "Verhardingstype opgebouwd uit bestratingen (rechthoekige of vierkante componenten) waardoor een aanzienlijk aantal voegen tussen de componenten zitten."@nl ;
      rdfs:isDefinedBy <http://data.vlaanderen.be/ns/openbaardomein/onbegroeidvoorkomen> ;
      rdfs:label "Bestrating Onbegroeid Voorkomen"@nl ;
      rdfs:subClassOf odo:OnbegroeidVoorkomen .

odo:GeslotenOnbegroeidVoorkomen
      a       owl:Class ;
      rdfs:comment "Verhardingstype gekenmerkt door een continue bedekking, waardoor er weinig voegen tussen de componenten aan te treffen zijn."@nl ;
      rdfs:isDefinedBy <http://data.vlaanderen.be/ns/openbaardomein/onbegroeidvoorkomen> ;
      rdfs:label "Gesloten Onbegroeid Voorkomen"@nl ;
      rdfs:subClassOf odo:OnbegroeidVoorkomen .

odo:OnbegroeidVoorkomen
      a       owl:Class ;
      rdfs:comment "Fysiek voorkomen van het aardoppervlak dat niet vegetatief is."@nl ;
      rdfs:isDefinedBy <http://data.vlaanderen.be/ns/openbaardomein/onbegroeidvoorkomen> ;
      rdfs:label "Onbegroeid Voorkomen"@nl ;
      rdfs:subClassOf <http://data.vlaanderen.be/ns/openbaardomein#TerreinVoorkomen> .

odo:bedekkingtype
      a       owl:ObjectProperty ;
      rdfs:comment "Het type bedekking."@nl ;
      rdfs:domain odo:BedekkingOnbegroeidVoorkomen ;
      rdfs:isDefinedBy <http://data.vlaanderen.be/ns/openbaardomein/onbegroeidvoorkomen> ;
      rdfs:label "bedekkingtype"@nl ;
      rdfs:range skos:Concept .

odo:bestratingVerhardingType
      a       owl:ObjectProperty ;
      rdfs:comment "Het type bestrating."@nl ;
      rdfs:domain odo:BestratingOnbegroeidVoorkomen ;
      rdfs:isDefinedBy <http://data.vlaanderen.be/ns/openbaardomein/onbegroeidvoorkomen> ;
      rdfs:label "bestrating verharding yype"@nl ;
      rdfs:range skos:Concept .

odo:geslotenVerhardingType
      a       rdf:Property ;
      rdfs:comment "Het type gesloten verharding."@nl ;
      rdfs:domain odo:GeslotenOnbegroeidVoorkomen ;
      rdfs:isDefinedBy <http://data.vlaanderen.be/ns/openbaardomein/onbegroeidvoorkomen> ;
      rdfs:label "gesloten verharding type"@nl .
