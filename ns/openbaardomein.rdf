  <owl:DatatypeProperty rdf:about="http://data.vlaanderen.be/ns/openbaardomein#einddatum">
    <rdfs:comment xml:lang="nl">Datum waarop de huidige versie van het gebied of object uit gebruik wordt genomen.</rdfs:comment>
    <rdfs:domain rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
